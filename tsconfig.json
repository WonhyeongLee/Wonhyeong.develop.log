{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "esnext"],
    "module": "commonjs",
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "strict": true,
    "allowUmdGlobalAccess": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "components/*": ["./components/*"],
      "utils/*": ["./utils/*"],
      "hooks/*": ["./hooks/*"],
      "types/*": ["./types/*"],
      "styles/*": ["./styles/*"],
      "pages/*": ["./pages/*"],
      "templates/*": ["./templates/*"],
      "context/*": ["./context/*"],
      "images/*": ["./images/*"]
    }
  },
  "include": [
    "./src/**/*",
    "./gatsby-node.ts",
    "./gatsby-config.ts",
    "./gatsby-browser.tsx",
    "./gatsby-ssr.tsx",
    "./plugins/**/*",
    "./src/gatsby-types.d.ts"
  ],
  "exclude": ["node_modules"]
}
